{
  "name": "staff-manager-fullstack",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd server && npm install",
    "dev": "concurrently \"npm run dev\" \"cd server && npm run dev\"",
    "build": "npm run build",
    "start:server": "cd server && npm start",
    "deploy:prep": "npm run build && cd server && npm install --production"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
